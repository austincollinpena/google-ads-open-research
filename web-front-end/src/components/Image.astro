---
export interface Props {
  Image: string;
  Alt: string;
  AspectRatio: string;
}
const { src, alt, AspectRatio } = Astro.props;

import { Image as AstroImage } from "@astrojs/image/components";
---

<AstroImage
  width={"1200px"}
  aspectRatio={AspectRatio}
  loading="lazy"
  alt={alt}
  src={src}
/>
